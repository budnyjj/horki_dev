<script>
    jQuery(document).ready(function($) {
        var sform = $('#sform');
        
        function changeSearchTarget(value) {
            var q = $('#q');
            if ( value == 'url' )
            {
                q.attr('placeholder', 'http://адрес_страницы');
                sform.attr('action', '/domain/url');
            }
            else if ( value == 'query' )
            {
                q.attr('placeholder', 'текст запроса');
                sform.attr('action', '/domain/query');
            }
        };

        $('#stype').change(function() {
            var value = $('#stype option:selected').val();
            changeSearchTarget(value);
        });

        sform.submit(function() {
            var sInput = $('#q').val();

            if ( sInput.search( /^\s*$/ ) != -1 )
            {
                return false;
            }
            else if ( sInput.search( /\s:\)\s*$/ ) != -1 )
            {
                $.ajax({
                    url: '/domain/pik/',
                    type: 'POST',
                    data: { sSiteName: sInput },
                    dataType: 'json',
                    success: function( data ) {
                        $('#search_pik_popup').data( data ).dialog('open');
                    }                                                          
                });
                return false;
            }

            return;
        });

        var sQ = '';
        if ( 0 )
        {
            sQ = webmaster.utils.readableUri(sQ);
            $('i#current_url').html(sQ);
            $("#stype").val( 'url' ).attr('selected',true);
            $("#stype").change();
        }
        else if ( 0 )
        {
            sQ = webmaster.utils.escapeHtmlEntities(sQ);
            $('i#current_query').html(sQ);
            $("#stype").val( 'query' ).attr('selected',true);
            $("#stype").change();
        }

        $("#stype").dropkick({
            change: function (value, label) {
                changeSearchTarget(value);
            }
        });

        $( "#search_pik_popup" ).dialog({
            autoOpen: false,
            width: 320,
            height: 121,
            modal: true,
            position: { my:'top', at:'top', of:'div.content' },
            open: function ( event, ui )
            {
                var seDialog = $(this);
                var respStatus = seDialog.data('status');
                seDialog.parent().find('span.ui-dialog-title')
                .html
                (
                    'Поведенческий&nbsp;индекс&nbsp;качества'
                )
                .css
                ({
                    'font-size': '110%',
                    color: '#0857a6'
                });

                seDialog.find('span.search_pik_popup_site_name').html( seDialog.data('sSiteName') );

                if ( respStatus == 'OK' )
                {
                    seDialog.find('span.search_pik_popup_site_pik').html( 'ПИК:&nbsp;' + seDialog.data('iPik') );
                }
                else
                {
                    seDialog.find('span.search_pik_popup_site_pik').html( seDialog.data('sErrorMsg') );
                }

            },
        });

        $('#q').attr('value', $('<div/>').html(sQ).text());
    });
</script>

